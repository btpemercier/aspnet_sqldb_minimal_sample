# Remove the line below if you want to inherit .editorconfig settings from higher directories
root = true

[*]
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
tab_width = 4
indent_size = 4
end_of_line = lf

# Code files
[*.{cs,csx,vb,vbx}]
indent_size = 4
